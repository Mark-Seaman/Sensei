#!/bin/bash
# Run the hourly task as seaman

if [ `hostname` == 'MyBook' ]
then
    export p=$HOME/MyBook
else
    export p=$HOME/Sensei
fi


{
    echo `date` `id` " - $0 Started"

    . $p/bin/bashrc
    # . $p/bin/venv

    python bin/x.py hourly run

    echo `date` " - $0 All done"
} 2>&1 > $p/log/cron-hourly.log

